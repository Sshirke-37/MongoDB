Q12. db.Film.aggregate([{$lookup: {from: "Actor",localField: "actors.fname",foreignField: "fname",as: "actor_info"}}])
Q13. db.Film.find({"director.fname": "Christopher","director.lname": "Nolan",year: { $gt: 2010 }})
Q14. db.Film.find({$expr: { $gt: [ { $size: "$release_details" }, 2 ] }})
Q15. 
Q16. db.Film.find({ release_details: { $elemMatch: { rating: { $gt: 8.5 } } } },{ _id: 0, title: 1, genre: 1 })
Q17. db.Film.find({"release_details.place": { $all: ["India", "USA"] }})
Q18. db.Actor.find({ fname: /^A/ })
Q19. db.Film.find({"actors.fname": {$in: ["Kate", "Alia", "Radhika", "Nina", "Anne", "Emma", "Isabel", "Lucia", "Zhang"]}})
Q20. db.Film.find({ genre: { $ne: "horror" } },{ _id: 0, title: 1, genre: 1 })
